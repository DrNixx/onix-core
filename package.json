    "@types/node": "^13.9.0",
    "@types/warning": "^3.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "esm": "^3.2.25",
    "mocha": "^7.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
